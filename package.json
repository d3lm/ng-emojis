{
  "name": "ng-emojis-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve",
    "start:debug": "npm run build:lib && node --inspect-brk ./node_modules/@angular/cli/bin/ng serve",
    "clean": "rimraf dist",
    "build:app": "ng build app",
    "build:lib": "tsc -p projects/ng-emojis/tsconfig.lib.json",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js -c jest.config.js --runInBand",
    "lint": "ng lint",
    "test:watch": "jest --watch"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/projects/ng-emojis/test.ts"
  },
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@briebug/jest-schematic": "^1.7.0",
    "@phenomnomnominal/tsquery": "^3.0.0",
    "fp-ts": "^1.19.2",
    "ngx-build-plus": "^8.1.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@testing-library/angular": "^7.1.0",
    "@types/jest": "^24.0.15",
    "@types/memory-fs": "^0.3.2",
    "@types/node": "^12.0.10",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.34",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "memfs": "^2.15.4",
    "memory-fs": "^0.4.1",
    "ng-packagr": "^5.1.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-cli": "^3.3.5"
